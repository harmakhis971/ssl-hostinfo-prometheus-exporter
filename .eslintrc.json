{
  "extends": "semistandard",
  "env": {
    "jasmine": true
  },
  "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
  },
  "plugins": [
    "jasmine"
  ],
  "rules": {
    "prefer-const": "error",
    "block-scoped-var": "error",
    "prefer-template": "warn",
    "no-use-before-define": [
      "error",
      "nofunc"
    ]
  }
}
